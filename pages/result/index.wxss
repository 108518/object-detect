canvas {
  background-color: #000;
  filter: blur(2px);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}

cover-view {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 20;
}

button {
  position: absolute;
  height: 80rpx;
  bottom: 80px;
  left: 0;
  right: 0;
  background-color: #d81e06;
  color: #fff;
  margin: auto;
}
